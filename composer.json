{
    "name": "chief/chief",
    "description": "A powerful standalone command bus package",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Nicholson",
            "email": "adamnicholson10@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "psr/log": "^1.0",
        "psr/cache": "^1.0",
        "illuminate/container": "*@dev",
        "illuminate/events": "*@dev",
        "league/container": "~1.3"
    },
    "suggest": {
        "psr/log": "Allows logging command execution",
        "psr/cache": "Allows caching the return value of commands"
    },
    "autoload": {
        "psr-4": {
            "Chief\\": ["src/", "tests/"]
        }
    }
}
